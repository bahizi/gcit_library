<div class="row row-centered">
	<div class="col-lg-12 col-centered">
		<div class="modal-header">
			<button type="button" class="close" data-dismiss="modal"
				aria-hidden="true">×</button>
			<h4 class="modal-title">Library Manager Login</h4>
		</div>

		<div class="modal-body">
			<div class="well col-lg-6">
				<form class="form-horizontal" action="borrowerLogin" method="post">
					<fieldset>
						<legend>Enter the branch you manage</legend>
						<div class="form-group">
							<label for="inputLibName" class="col-lg-2 control-label">Library</label>
							<div class="col-lg-10">
								<input type="text" class="form-control" name="libName"
									id="inputLibName" placeholder="Library Name">
							</div>
						</div>					
						<div class="form-group">
							<div class="col-lg-10 col-lg-offset-2">
								<button type="button" class="btn btn-default"
									data-dismiss="modal">Close</button>
								<button type="submit" class="btn btn-primary">Submit</button>
							</div>
						</div>
					</fieldset>
				</form>
			</div>
			<div class="col-lg-6 col-centered">
			<div class="bs-component">				
					<table id="selectTable" class="table table-striped table-hover">

					</table>
					<div class="row">
						<div class="col-lg-4 col-lg-offset-4">
							<div class="bs-component">
								<ul id="pagination" class="pagination">
								</ul>
							</div>
						</div>
					</div>
				</div>			
			</div>
			
		</div>
	</div>
</div>
